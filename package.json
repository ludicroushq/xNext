{
  "name": "TODO",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@xframework/config": "^0.4.0",
    "@xframework/x": "^0.4.0",
    "lucide-react": "^0.509.0",
    "next": "^15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.34.1",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.1.3",
    "@tailwindcss/postcss": "^4.1.6",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "daisyui": "^5.0.35",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "neostandard": "^0.12.0",
    "postcss": "^8.5.3",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^4.1.6",
    "typescript": "^5.7.2",
    "wireit": "^0.14.12"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --ignore-unknown --write"
    ]
  },
  "scripts": {
    "build": "wireit",
    "test:ci": "wireit",
    "dev": "wireit",
    "prepare": "husky",
    "start": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "next:build"
      ]
    },
    "dev": {
      "dependencies": [
        "next:dev"
      ]
    },
    "eslint:check": {
      "command": "eslint --max-warnings 0 ."
    },
    "eslint:fix": {
      "command": "eslint --fix --max-warnings 0 ."
    },
    "next:build": {
      "command": "next build",
      "output": [
        ".next/**"
      ],
      "clean": false
    },
    "next:dev": {
      "command": "next dev --turbo",
      "service": true
    },
    "next:start": {
      "command": "next start",
      "service": true
    },
    "prettier:check": {
      "command": "prettier --check ."
    },
    "prettier:write": {
      "command": "prettier --write ."
    },
    "start": {
      "dependencies": [
        "next:start"
      ]
    },
    "test": {
      "dependencies": [
        "eslint:fix",
        "prettier:write",
        "typescript:check"
      ]
    },
    "test:ci": {
      "dependencies": [
        "eslint:check",
        "prettier:check",
        "typescript:check"
      ]
    },
    "typescript:check": {
      "command": "tsc --noEmit"
    }
  }
}
